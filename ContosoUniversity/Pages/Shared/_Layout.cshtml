                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                University Demo

                            </a>
                            <div class="dropdown-menu" aria-labelledby="navbarDropdown">

                                <a class="nav-link text-light" asp-area="" asp-page="/Students/Index">Students</a>
                                <a class="nav-link text-light" asp-area="" asp-page="/Courses/Index">Courses</a>
                                <a class="nav-link text-light" asp-area="" asp-page="/Instructors/Index">Instructors</a>
