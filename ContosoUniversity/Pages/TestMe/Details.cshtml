@page
@model ContosoUniversity.Pages.TestMe.DetailsModel
@{
}
@{
    ViewData["Title"] = "Students";
}

<h2>Logs for @Model.LogItem</h2>

<a asp-page="./testme/Index">Back to List</a>

@{
    int len = Model.blow.Length;
    if (len != 0)
    {
        for (int i = 0; i < Model.blow.Length; i++)
        {
            string j = @Model.blow[i];

            <h4>@j</h4>
        }
    }
    else
    {
        <h4>No Logs found</h4>
    }
}
 